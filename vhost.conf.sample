[apache]
dir_hosts_enabled = /etc/apache2/sites-enabled
dir_hosts_available = /etc/apache2/sites-available
restart_command = service apache2 reload

[general]
vhost_file_suffix = .lan.conf
domain      = .lan
sites_dir = /var/www
hosts_file = /etc/hosts
user = www-data
group = www-data

[mysql]
user = root
password = pass
host = localhost
charset = utf8

[ssl]
cert_file = /etc/apache2/ssl/default.crt
key_file = /etc/apache2/ssl/default.key